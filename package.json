{
  "name": "create-package-name",
  "description": "Create a new npm package boilerplate",
  "version": "0.0.0",
  "author": "Jakub Grzywacz",
  "bin": {
    "create-package-name": "./bin/run.js"
  },
  "bugs": "https://github.com/https://github.com/jakex7/create-package-name/issues",
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "inquirer": "^12.10.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/https://github.com/jakex7/create-package-name",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "create-package-name",
    "dirname": "create-package-name",
    "commands": {
      "strategy": "single",
      "target": "./dist/index.js"
    },
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/jakex7/create-package-name",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
